[
  {
    "software": {
      "bundle_identifier": ["us.zoom.xos"],
      "source": ["apps"]
    },
    "filter": {
      "product": ["zoom", "meetings"],
      "vendor": ["zoom"],
      "target_sw": ["macos", "mac_os"]
    }
  },
  {
    "software": {
      "name": ["/^ruby(@.*)?$/"],
      "source": ["homebrew_packages"]
    },
    "filter": {
      "product": ["ruby"],
      "vendor": ["ruby-lang"]
    }
  },
  {
    "software": {
      "name": ["/^node(@.*)?$/"],
      "source": ["homebrew_packages"]
    },
    "filter": {
      "product": ["node.js"],
      "vendor": ["nodejs"]
    }
  }
]
