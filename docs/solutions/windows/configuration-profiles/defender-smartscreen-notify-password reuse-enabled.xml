<Replace>
    <!-- Enable Defender SmartScreen to warn users if reusing their work or school
    password -->
    <Item>
        <Meta>
            <Format xmlns="syncml:metinf">int</Format>
        </Meta>
        <Target>
            <LocURI>./Device/Vendor/MSFT/Policy/Config/WebThreatDefense/NotifyPasswordReuse</LocURI>
        </Target>
        <Data>1</Data>
    </Item>
</Replace>